{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "fs-blocks/modal",
  "version": "0.1.0",
  "title": "Bootstrap Modal",
  "category": "widgets",
  "icon": "feedback",
  "description": "A Bootstrap 5 Modal dialog. Set the same ID as your Modal Button.",
  "keywords": [
    "bootstrap",
    "modal"
  ],
  "textdomain": "fs-blocks",
  "supports": {
    "html": false,
    "anchor": true
  },
  "attributes": {
    "modalId": {
      "type": "string",
      "default": "myModal"
    },
    "modalTitle": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}