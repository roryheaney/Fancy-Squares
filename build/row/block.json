{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "fs-blocks/row-block",
  "version": "0.1.0",
  "title": "Fs Row",
  "category": "layout",
  "icon": "screenoptions",
  "keywords": [
    "bootstrap",
    "row",
    "layout"
  ],
  "description": "A row block that can hold a column or grid block.",
  "supports": {
    "html": false,
    "anchor": true
  },
  "attributes": {
    "rowOptions": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "justifyContentOptions": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "alignItemsOptions": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "marginOptions": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "paddingOptions": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "additionalClasses": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    }
  },
  "textdomain": "fs-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}