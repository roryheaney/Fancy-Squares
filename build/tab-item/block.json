{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "fs-blocks/tab-item",
  "version": "0.1.0",
  "title": "Tab Item",
  "category": "layout",
  "parent": [
    "fs-blocks/tabs"
  ],
  "icon": "admin-page",
  "description": "Single tab content pane",
  "textdomain": "fs-blocks",
  "supports": {
    "html": false,
    "anchor": true,
    "__experimentalLayout": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "New Tab"
    },
    "tabId": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}