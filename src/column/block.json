{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fs-blocks/column-block",
	"version": "0.1.0",
	"title": "Fs Column",
	"category": "layout",
	"icon": "screenoptions",
	"keywords": [ "bootstrap", "column", "layout" ],
	"description": "A column block that can hold inner blocks.",
	"supports": {
		"html": false,
		"anchor": true
	},
	"attributes": {
		"singularSelectClass": {
			"type": "string",
			"default": ""
		},
		"columnOptions": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"default": []
		},
		"marginOptions": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"default": []
		},
		"displayOptions": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"default": []
		},
		"orderOptions": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"default": []
		},
		"selfAlignmentOptions": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"default": []
		},
		"columnOffsetOptions": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"default": []
		},
		"additionalClasses": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"default": []
		}
	},
	"textdomain": "fs-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
