{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fs-blocks/container-block",
	"version": "0.1.0",
	"title": "Fs Container",
	"category": "layout",
	"icon": "screenoptions",
	"description": "A container block that can hold row blocks or other inner blocks.",
	"keywords": [ "bootstrap", "container", "layout" ],
	"supports": {
		"html": false,
		"anchor": true
	},
	"attributes": {
		"additionalClasses": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"default": []
		}
	},
	"textdomain": "fs-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
